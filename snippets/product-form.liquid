    margin: 29% 19% 11% 21%;
    /* margin: 25% 20% 15%; */
    aspect-ratio: 1;
    align-items: center;
    justify-content: center;
}
@media only screen and (max-width: 481px) {
    .initials-color1-text {
      /*height: 310px;*/
      width: 60%;
      aspect-ratio: 1;
