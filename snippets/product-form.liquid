    clip: rect(0,0,0,0);
    overflow: hidden;
    position: absolute;
    height: 1px;
    width: 1px;
  }  
  .text-color-label {
    border: 1px transparent solid;
    width: 1.5rem;
    height: 1.5rem;
    padding: 0.25rem;
    overflow: hidden;
    border-radius: 25px;
    margin: 5px;
    text-indent: 100%;
    white-space: nowrap;
    cursor: pointer;
  }
  input[ type= 'radio'].text-color-input:checked + label {
    border-color: rgb(255, 100, 145);
